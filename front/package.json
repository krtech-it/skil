{
  "name": "atomskills2024-training",
  "version": "0.0.1",
  "description": "",
  "repository": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "mock": "vite --mode mock",
    "mockserver": "nodemon -e js,json -x node --inspect --trace-warnings server/src/index.js",
    "build": "vite build",
    "check": "run-p lint test",
    "check-eslint-and-prettier-conflicts": "eslint --print-config src/index.tsx | eslint-config-prettier-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "COVERAGE_DIR='reports/unit-tests-coverage' npm test -- --coverage --ci",
    "test-backstop": "backstop test --config='configs/backstop_data/config.js' --docker",
    "test-backstop-approve": "backstop approve --config='configs/backstop_data/config.js' --docker",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx src --fix",
    "lint:codestyle": "eslint '*.json' '*.js' '.*.js' configs src --ext .js,.json,.ts,.tsx",
    "lint:pretty": "run-p lint:pretty:es lint:pretty:json",
    "lint:pretty:es": "npm run _pretty -- --check '**/*.{js,ts,tsx}'",
    "lint:pretty:json": "npm run _pretty:json -- --check '**/*.json'",
    "lint:types": "tsc",
    "pretty": "run-p pretty:es pretty:json",
    "pretty:es": "npm run _pretty -- --write '**/*.{js,ts,tsx,d.ts}'",
    "pretty:json": "npm run _pretty:json:write -- '**/*.json'",
    "_pretty": "prettier --write ./src",
    "_pretty:json": "npm run _pretty -- --tab-width 2",
    "_pretty:json:write": "npm run _pretty:json -- --write",
    "lint:css": "stylelint \\\"./src/**/*.ts\\\"",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@quark-uilib/components": "^1.13.7",
    "@quark-uilib/icons": "^1.2.1",
    "@quark-uilib/table": "1.3.5",
    "@react-pdf/renderer": "^3.4.4",
    "axios": "1.6.2",
    "axios-cache-interceptor": "1.3.2",
    "date-fns": "3.6.0",
    "date-fns-tz": "^3.1.3",
    "mobx": "6.12.0",
    "mobx-react": "9.1.0",
    "mobx-react-lite": "4.0.5",
    "nprogress": "0.2.0",
    "paper": "^0.12.17",
    "polished": "4.2.2",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router": "6.20.1",
    "react-router-dom": "6.20.1",
    "recharts": "^2.12.7",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "styled-components": "5.3.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@quark-uilib/dotfiles": "1.0.2",
    "@types/node": "18.19.2",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.34",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "@vitejs/plugin-react": "4.2.1",
    "babel-plugin-styled-components": "2.1.4",
    "eslint": "8.22.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-optimize-regex": "1.2.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-html-reporter": "3.10.2",
    "json-server": "0.17.4",
    "lint-staged": "12.3.1",
    "nodemon": "3.0.2",
    "prettier": "3.1.0",
    "stylelint": "15.11.0",
    "stylelint-config-recommended": "13.0.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "typescript": "4.5.5",
    "vite": "^5.0.5",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "resolutions": {
    "styled-components": "^5",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2"
  },
  "private": true
}
